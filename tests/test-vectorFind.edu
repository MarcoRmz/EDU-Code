start
int a[3]

function bool vectorFind(int n, int size){
	int i
	i = 0
	while( i < size){
		if(a[i] == n){
			return true
		}
		i = i + 1
	}
	return false
}

main{
	int y
	int z
	bool x
	bool m
	y = 7
	z = 12
	a[0] = 3
	a[1] = 7
	a[2] = 9

	x = vectorFind(y,3)
	print("Resultado vector find para: " + y + " es: " + x)

	m = vectorFind(z,3)
	print("Resultado vector find para: " + z + " es: " + m)
}

end
